console.log("even numbers between 1 and 100 ")
//with for
for(let i = 1; i <= 100; i++){
    if(i % 2 == 0){
        console.log(i)
    }
}
//whith while
let i = 1
while(i <= 100){
    if(i % 2 == 0){
        console.log(i)
    }
    i++
}
//whith do while
let num = 1
do{
    if(num % 2 == 0){
        console.log(num)
    }
    num++
}while(num <= 100);

