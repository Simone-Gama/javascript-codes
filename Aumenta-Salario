let salario = 1500

if(salario <= 2000){
    let sal_aumenta =  salario + (salario * 0.10)
    console.log("Sal치rio < 2000")
    console.log("Novo Sal치rio:", sal_aumenta)
}else{
    let sal_aumenta =  salario + (salario * 15/100)
    console.log("Sal치rio > 2000")
    console.log("Novo Sal치rio:", sal_aumenta)
}
